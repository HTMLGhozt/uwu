{
    "name": "@uwu/backend",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "start": "node lib/server",
        "clean": "rm -rf lib",
        "build": "npm -s run clean && npm -s run generate && tsc",
        "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
        "generate:prisma": "prisma2 generate",
        "generate:nexus": "ts-node --transpile-only src/schema",
        "prepare": "DATABASE_URL=postgresql://postgres:uwu@localhost/postgres npm -s run generate",
        "test": "which prisma2",
        "dev": "concurrently -n server,primsa -c 'blue.bold,magenta.bold' 'yarn dev:server' 'yarn dev:prisma'",
        "dev:server": "ts-node-dev --no-notify --respawn --transpileOnly src/server",
        "dev:prisma": "/usr/local/bin/prisma2 dev",
        "migrate": "prisma2 lift up"
    },
    "dependencies": {
        "@jordangens/twitch-pubsub-client": "^3.7.2",
        "@prisma/photon": "2.0.0-preview018.2",
        "@types/connect-redis": "^0.0.13",
        "@types/node-fetch": "^2.5.4",
        "@types/redis": "^2.8.14",
        "apollo-server-express": "^2.9.13",
        "connect-redis": "^4.0.3",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "express-session": "^1.17.0",
        "graphql": "14.5.8",
        "ioredis": "^4.14.1",
        "nexus": "0.12.0-rc.5",
        "nexus-prisma": "0.6.1",
        "passport": "^0.4.1",
        "passport-twitch.js": "^0.0.7",
        "twitch": "^3.5.6",
        "twitch-chat-client": "^3.6.0",
        "twitch-pubsub-client": "^3.5.9",
        "twitch-webhooks": "^3.5.6",
        "ws": "^7.2.1"
    },
    "devDependencies": {
        "@prisma/sdk": "0.0.100",
        "@types/express-session": "^1.15.16",
        "@types/node": "12.12.18",
        "@types/passport": "^1.0.2",
        "@types/ws": "6.0.4",
        "concurrently": "^5.0.1",
        "prisma2": "2.0.0-preview018.2",
        "ts-node": "8.5.4",
        "ts-node-dev": "1.0.0-pre.44",
        "typescript": "3.7.3"
    }
}
